---------------------------------------------------------------
-- REORDER REPORT TABLE
---------------------------------------------------------------
CREATE TABLE INVENTORY.REORDER_REPORT (
  ITEMID        CHAR(10),
  ITEMDESC      VARCHAR(100),
  QTY_ON_HAND   DECIMAL(15,2),
  REORDER_LEVEL DECIMAL(15,2),
  LOCATION      CHAR(10),
  GENERATION_TS TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);

