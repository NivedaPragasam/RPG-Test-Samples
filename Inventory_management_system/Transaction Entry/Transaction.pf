---------------------------------------------------------------
-- TRANSACTIONS TABLE
---------------------------------------------------------------
CREATE TABLE INVENTORY.TRANSACTIONS (
  TRANSID      BIGINT GENERATED ALWAYS AS IDENTITY PRIMARY KEY,
  ITEMID       CHAR(10) NOT NULL,
  TRANS_TYPE   CHAR(1) NOT NULL,
  QTY          DECIMAL(15,2) NOT NULL,
  LOCATION     CHAR(10),
  REFNO        VARCHAR(50),
  USERID       VARCHAR(50),
  TRANS_DATE   TIMESTAMP DEFAULT CURRENT_TIMESTAMP
);
